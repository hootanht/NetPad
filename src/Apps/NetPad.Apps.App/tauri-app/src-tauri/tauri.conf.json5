{
    "productName": "NetPad",
    "version": "0.8.0",
    "identifier": "com.netpad.app",
    "build": {
        "devUrl": "http://localhost:57940",
        "beforeDevCommand": {
            "script": "npm run start-web",
            "cwd": "../../App"
        },
        "beforeBuildCommand": "",
        "frontendDist": "http://localhost:57930"
    },
    "app": {
        "windows": [
            {
                "title": "NetPad",
                "maximized": true,
                "width": 1400,
                "height": 800,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "resources": {
            "../../bin/Release/net6.0/linux-x64/publish": "resources/netpad-server"
        }
    },
    "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}
